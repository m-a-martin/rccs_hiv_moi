	number = {7},
	journal = {The Journal of Infectious Diseases},
	author = {Wagner, Gabriel A. and Pacold, Mary E. and Kosakovsky Pond, Sergei L. and Caballero, Gemma and Chaillon, Antoine and Rudolph, Abby E. and Morris, Sheldon R. and Little, Susan J. and Richman, Douglas D. and Smith, Davey M.},
