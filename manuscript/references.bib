	pages = {267--274},
	file = {Redd et al. - 2012 - The Rates of HIV Superinfection and Primary HIV In.pdf:/Users/mmart59/Zotero/storage/47SX5I7W/Redd et al. - 2012 - The Rates of HIV Superinfection and Primary HIV In.pdf:application/pdf},
